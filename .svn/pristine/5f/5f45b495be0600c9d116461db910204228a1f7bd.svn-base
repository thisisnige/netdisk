package com.iamnige.database;

public class DBField {
	public String key;
	public Object value;
	public String type;
	public String operator;
	public DBField(String key, Object value, String type, String op){
		this.key = key;
		this.value = value;
		this.type = type;
		this.operator = op;
	}
	
	public DBField(String key, String type){
		this.key = key;
		this.value = null;
		this.type = type;
		this.operator = "=";
	}
	
	public DBField(String key, Object value, String type){
		this.key = key;
		this.value = value;
		this.type = type;
		this.operator = "=";
	}
}
