@CHARSET "UTF-8";

body, button, input, select, textarea , a, table{
font: 12px/1.5 tahoma,arial,"宋体";
color: #666;
}

ul,li{margin:0;padding:0; list-style:none;}
.menu{width:100%;height:49px;margin:0 auto;background: url(../img/nav_bg_1.jpg) repeat-x;}
.menu ul{width:100%;float:left;height:49px;display:inline;}
.menu ul li{width:100px;height:36px; line-height:36px; text-align:center;margin:5px 0; float:right;display:inline;}
.menu ul li a{ text-decoration:none; width:100px; height:36px; line-height:36px; float:left;color:#fff; font-size:12px}
.menu ul li a:hover{ text-decoration:underline;}

.uploader { position:relative; display:inline-block; overflow:hidden; cursor:default; padding:0; margin:10px 0px; }
.filename { float:left; display:inline-block; outline:0 none; height:28px; width:180px; margin:0; padding:8px 10px; overflow:hidden; cursor:default; border:1px solid; border-right:0; color:#777; text-shadow:1px 1px 0px #fff; text-overflow:ellipsis; white-space:nowrap; -moz-border-radius:5px 0px 0px 5px; -webkit-border-radius:5px 0px 0px 5px; border-radius:5px 0px 0px 5px; background:#f5f5f5; background:-moz-linear-gradient(top, #fafafa 0%, #eee 100%); background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #fafafa), color-stop(100%, #f5f5f5)); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#fafafa', endColorstr='#f5f5f5', GradientType=0);
border-color:#ccc; -moz-box-shadow:0px 0px 1px #fff inset; -webkit-box-shadow:0px 0px 1px #fff inset; box-shadow:0px 0px 1px #fff inset; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box; }
.button { float:left; height:28px; display:inline-block; outline:0 none;  margin:0; cursor:pointer; border:1px solid;  -moz-border-radius:0px 5px 5px 0px; -webkit-border-radius:0px 5px 5px 0px; border-radius:0px 5px 5px 0px; -moz-box-shadow:0px 0px 1px #fff inset; -webkit-box-shadow:0px 0px 1px #fff inset; box-shadow:0px 0px 1px #fff inset; }
.button_all_radis {-moz-border-radius:5px 5px 5px 5px; -webkit-border-radius:5px 5px 5px 5px; border-radius:5px 5px 5px 5px;float:left; height:28px; display:inline-block; outline:0 none;  margin:0; cursor:pointer; border:1px solid;  }
.uploader input[type=file] { width:270px; position:absolute; top:0; left:0; bottom:0; border:0; padding:0; margin:0; height:30px; cursor:pointer; filter:alpha(opacity=0); -moz-opacity:0; -khtml-opacity: 0; opacity:0; }
 input[type=button]::-moz-focus-inner {
padding:0;
border:0 none;
-moz-box-sizing:content-box;
}
input[type=button]::-webkit-focus-inner {
padding:0;
border:0 none;
-webkit-box-sizing:content-box;
}
input[type=text]::-moz-focus-inner {
padding:0;
border:0 none;
-moz-box-sizing:content-box;
}
input[type=text]::-webkit-focus-inner {
padding:0;
border:0 none;
-webkit-box-sizing:content-box;
}

.black .button, .black .button_all_radis { color:#fff; text-shadow:1px 1px 0px #111111; background:#222222; background:-moz-linear-gradient(top, #444444 0%, #222222 100%); background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #444444), color-stop(100%, #222222)); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#444444', endColorstr='#222222', GradientType=0);
border-color:#111111; }

.title {
background: #F7F7F7;
border-radius: 2px;
border: 1px solid #D2D2D2;
color: #888;
height: 41px;
line-height: 41px;
border-left: 1px solid #fff;
border-right: 1px solid #e5e5e5;
}

.item {
border-bottom: 1px solid #D2D2D2;
}

.global-clearfix {
zoom: 1;
}

.c1 {
position: relative;
text-align: left;
top: 0;
overflow: visible;
}

.col {
float: left;
height: 38px;
line-height: 38px;
text-indent: 10px;
width: 10%;
white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;
border-right: 1px solid #e5e5e5;
}
.list{
	border-radius: 2px;
border-bottom: 1px solid #D2D2D2;
color: #888;
height: 41px;
line-height: 41px;
border-left: 1px solid #fff;
border-right: 1px solid #e5e5e5;
}

.global-icon-16 {
display: inline-block;
width: 26px;
height: 22px;
background: url(../img/sprite_list_icon.gif) 9999px 9999px no-repeat;
background-position: -421px -83px;
}

.global-icon-16-dir {
background-position: -5px -3px;
}

.global-icon-16-txt {
background-position: -165px -83px;
}

.ico {
position: absolute;
top: 12px;
left: 10px;
}

.list .name {
width: 100%;
white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;
text-indent: 35px;
font-family: simsun;
line-height:38px;
}
